# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# CORS Settings (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173


# ==============================================
# CACHE CONFIGURATION
# ==============================================
CACHE_TTL_SECONDS=3600
CACHE_THRESHOLD=0.5
SEMANTIC_CACHE_FILE=semantic_cache.json


# ==============================================
# RATE LIMITING
# ==============================================
# Default query limit per month for free users
